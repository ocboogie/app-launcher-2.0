{
  "name": "app-launcher-2.0",
  "version": "0.2.2",
  "description": "A customizable app launcher",
  "main": "dist/main",
  "scripts": {
    "start": "gulp set-dev && electron dist/main/main.js",
    "build": "gulp",
    "watch": "npm run build && gulp watch",
    "package:win": "gulp set-prod && electron-packager . \"App Launcher\" --overwrite --platform=win32 --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocboogie/app-launcher-2.0.git"
  },
  "author": "ocboogie",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-flow-runtime": "^0.10.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "electron": "^1.6.1",
    "electron-packager": "^8.5.2",
    "flow-runtime": "^0.10.0",
    "flow-typed": "^2.0.0",
    "redux-logger": "^2.8.2"
  },
  "dependencies": {
    "chokidar": "^1.6.1",
    "electron-is-accelerator": "^0.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
